@model Sipecom.FactElec.Pymes.Servicios.Api.Identidad.Models.Account.LogoutViewModel

<div class="logout-page">
    <div class="lead">
        <h1>¿Desa cerrar sesión?</h1>
    </div>

    <form asp-action="Logout">
